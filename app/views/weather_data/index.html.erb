<p id="notice"><%= notice %></p>

<h1>Weather Data</h1>

<table>
  <thead>
    <tr>
      <th>Timestamp</th>
      <th>Zipcode</th>
      <th>Temperature</th>
    </tr>
  </thead>

  <tbody>
    <% @weather_data.each do |weather_datum| %>
      <tr>
        <td><%= weather_datum.created_at %></td>
        <td><%= weather_datum.zipcode %></td>
        <td>
          <%= weather_datum.temperature_f %>F /
          <%= weather_datum.temperature_c %>C
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weather Datum', new_weather_datum_path %>
